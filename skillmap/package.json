{
  "name": "skillsmap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "8.0.53",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.3",
    "react": "16.8.3",
    "react-dom": "16.11.0",
    "react-redux": "5.1.1",
    "react-scripts": "3.1.0",
    "redux": "3.7.2",
    "typescript": "4.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "mv node_modules/react-scripts/config/webpack.config.js node_modules/react-scripts/config/webpack.config.base.js && cp webpack.config.override.js node_modules/react-scripts/config/webpack.config.js",
    "build": "react-scripts build",
    "postbuild": "mv node_modules/react-scripts/config/webpack.config.base.js node_modules/react-scripts/config/webpack.config.js && rm build/static/js/runtime* && rm build/static/js/*.txt"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
